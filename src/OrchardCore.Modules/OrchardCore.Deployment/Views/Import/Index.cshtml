<zone Name="Title"><h1>@RenderTitleSegments(T["Import Deployment Package"])</h1></zone>

<form asp-action="Import" enctype="multipart/form-data">
    <nav class="card">
        <ul class="navbar-nav card-body bg-primary">
            <li class="nav-item">
                <div class="mb-3">
                    <input type="file" name="importedPackage" class="form-control" accept=".zip,.json" />
                </div>
                <button class="btn btn-primary import" type="submit">@T["Import"]</button>
            </li>
        </ul>
    </nav>
</form>
